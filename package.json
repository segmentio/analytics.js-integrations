{
  "name": "analytics.js-integrations",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/segmentio/analytics.js-integrations.git",
  "author": "Segment",
  "license": "See LICENSE",
  "private": true,
  "workspaces": [
    "integrations/*"
  ],
  "scripts": {
    "clean": "rm -rf *.log coverage bin/* node_modules",
    "coverage": "codecov",
    "fmt": "standard --fix",
    "lint": "eslint integrations/**/lib/ integrations/**/test/",
    "publish-integrations": "scripts/publish-integrations",
    "test-all-integrations": "karma start karma.conf.js all",
    "test-integration": "karma start karma.conf.js",
    "test-updated-integrations": "scripts/test-integrations",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint"
      ]
    }
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "devDependencies": {
    "@segment/analytics.js-core": "^3.7.2",
    "@segment/analytics.js-integration-tester": "^3.1.1",
    "@segment/clear-env": "^2.1.1",
    "@segment/eslint-config": "^4.0.0",
    "browserify": "^16.2.3",
    "browserify-istanbul": "^3.0.1",
    "eslint": "^5.6.1",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-require-path-exists": "^1.1.9",
    "husky": "^1.1.2",
    "karma": "^3.0.0",
    "karma-browserify": "^5.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.2.0",
    "karma-spec-reporter": "^0.0.32",
    "lint-staged": "^7.3.0",
    "mocha": "^2",
    "nyc": "^13.0.1",
    "phantomjs-prebuilt": "^2.1.16",
    "sinon": "^6.3.5",
    "watchify": "^3.11.0"
  }
}
